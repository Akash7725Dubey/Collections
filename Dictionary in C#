class Program
    {
        public static void Main(string[] args)
        {
            Dictionary<string,int> dic = new Dictionary<string,int>();
            dic.Add("one", 1);
            dic.Add("two", 2);
            dic.Add("three", 3);
            dic.Add("four", 4);
            foreach(var value in dic)
            {
                Console.WriteLine(value);
            }
        }
    }
}
